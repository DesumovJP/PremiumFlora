{
  "kind": "collectionType",
  "collectionName": "supplies",
  "info": {
    "singularName": "supply",
    "pluralName": "supplies",
    "displayName": "Supply",
    "description": "Supply records from Excel imports and manual entries"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "filename": {
      "type": "string",
      "required": true
    },
    "checksum": {
      "type": "string"
    },
    "dateParsed": {
      "type": "datetime"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "awb": {
      "type": "string"
    },
    "supplier": {
      "type": "string"
    },
    "rows": {
      "type": "json"
    },
    "supplyStatus": {
      "type": "enumeration",
      "enum": [
        "success",
        "failed",
        "dry-run"
      ]
    },
    "supplyErrors": {
      "type": "json"
    },
    "supplyWarnings": {
      "type": "json"
    },
    "costCalculationMode": {
      "type": "enumeration",
      "enum": [
        "simple",
        "full"
      ],
      "default": "simple"
    },
    "fullCostParams": {
      "type": "json"
    }
  }
}
